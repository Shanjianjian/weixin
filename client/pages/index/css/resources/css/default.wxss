body {
  font-family : Arial, Helvetica, Sans-Serif;
  background-color: #8f949a;
  overflow:hidden;
}
a {
  cursor: pointer;
}

input[type="radio"], input[type="checkbox"] {
    margin: 0rpx 18rpx 0rpx 0rpx;
    vertical-align: bottom;
}

input {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

#maximizePanelContainer {
  font-size: 8rpx;
  position:absolute;
  left: 0rpx;
  top: 0rpx;
  width: 110rpx;
  height: 40rpx;
  overflow: visible;
  z-index: 1000;
}
#maximizePanelOver {
    position: absolute;
    left: 0rpx;
    top: 0rpx;
    width: 110rpx;
    height: 40rpx;
}
.maximizePanel {
    position: absolute;
    left: 0rpx;
    top: 0rpx;
    width: 110rpx;
    height: 40rpx;
    background: #a2a2a2 url('../images/expand.png') no-repeat center center;
    background: url('../images/expand.svg') no-repeat center center, linear-gradient(rgba(200,200,200,.5),rgba(200,200,200,.5));
    cursor: pointer;
}

#interfaceControlFrameMinimizeContainer {
  position:absolute;
    top: 0rpx;
    left: 0rpx;
  font-size: 4rpx; /*for IE*/
  text-align: right;
  z-index: 100;
    height: 40rpx;
    width: 110rpx;
    background-color: #62666b;
}
#interfaceControlFrameMinimizeContainer a {
    display: inline-block;
    width: 110rpx;
    height: 40rpx;
    font-size: 4rpx;
    background: url('../images/close.png') no-repeat center center;
    background: url('../images/close.svg') no-repeat center center, linear-gradient(transparent,transparent);
    text-decoration: none;
}

.hashover #interfaceControlFrameMinimizeContainer a:hover {
    background: url('../images/close_hover.png') no-repeat center center;
    background: url('../images/close_hover.svg') no-repeat center center, linear-gradient(transparent,transparent);
}

#interfaceControlFrame {
    margin: 0rpx 0rpx 0rpx 110rpx;
}

#interfaceControlFrameCloseContainer {
    /*display: none;*/
    position:absolute;
    bottom: 0rpx;
    left: 0rpx;
    font-size: 18rpx;
    font-family: 'Trebuchet MS';
    font-weight: bold;
    letter-spacing: 2rpx;
    z-index: 100;
    width: 110rpx;
    background-color: #62666b;
    text-align: center;  
}
#interfaceControlFrameCloseContainer a {
    display: inline-block;
    width: 110rpx;
    color: #ffffff;
    padding: 10rpx 0rpx;
}

#interfaceControlFrameHeader li a {
    display: block;
    width: 108rpx;
    height: 156rpx;
    text-align: center;
    padding-top: 100rpx;
    outline: none;
    margin-right: 2rpx;
    text-decoration: none;
    color: #ffffff;
    white-space: nowrap;
    background-color: transparent;
    background-repeat: no-repeat;
    background-position: 50% 34rpx;
    box-sizing: border-box;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    border-left: 8rpx solid transparent;
    border-right: 8rpx solid transparent;
}

.hashover #interfaceControlFrameHeader li a:hover {
    background-color: transparent;
    background-repeat: no-repeat;
    background-position: 50% 34rpx;
    color: #c2c2c2;
}

#interfaceControlFrameHeader li a.selected, #interfaceControlFrameHeader li a.selected:hover {
    background-color: #f5f5f5;
    background-repeat: no-repeat;
    background-position: 50% 34rpx;
    color: #62666b;
    border-left: 10rpx solid #138CDD;
}

#interfaceControlFrameHeaderContainer {
    float: left;
    overflow: visible;
    width: 110rpx;
    margin-left: -110rpx;
    margin-top: 40rpx;
}

#interfaceControlFrameHeader {
    position: relative;
    list-style: none;
    font-size: 16rpx;
    z-index: 50;
    font-family: 'Trebuchet MS';
    font-weight: bold;
    letter-spacing: 2rpx;
}

#interfaceControlFrameContainer {
    float: right;
    background-color: #f5f5f5;
    overflow: hidden;
    width: 100%;
}

#interfaceControlFrameLogoContainer {
  background-color: White;
  padding: 40rpx 20rpx 20rpx 20rpx;
  overflow: hidden;
}

#interfaceControlFrameLogoImageContainer {
  text-align: center;
}

#interfaceControlFrameLogoCaptionContainer {
  text-align: center;
  margin: 10rpx 20rpx 0rpx 20rpx;
  font-size: 24rpx;
  color: #4a4a4a;
}

#logoImage {
    width: 100%;
}

.emptyStateContainer {
    text-align: center;
    padding: 0rpx 20rpx;
    margin-top: 64rpx
}

.emptyStateTitle {
    margin: 0rpx 0rpx 18rpx 0rpx;
    font-weight: bold;
}

.emptyStateContent {
    line-height: 32rpx;
}

.dottedDivider {
    height: 4rpx;
    margin: 30rpx 0rpx 30rpx 0rpx;
    background: url('../images/divider.png') no-repeat center center;
    background: url('../images/divider.svg') no-repeat center center, linear-gradient(transparent,transparent);
}

.nondottedDivider {
    height: 4rpx;
    margin: 18rpx 0rpx 18rpx 0rpx;
}